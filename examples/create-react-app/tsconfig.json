{
	"compilerOptions": {
		"target": "es5",
		"lib": ["dom", "dom.iterable", "esnext"],
		"allowJs": true,
		"skipLibCheck": true,
		"strict": false,
		"strictNullChecks": true,
		"forceConsistentCasingInFileNames": true,
		"noEmit": true,
		"incremental": true,
		"esModuleInterop": true,
		"module": "esnext",
		"resolveJsonModule": true,
		"isolatedModules": true,
		"jsx": "preserve",
		"moduleResolution": "node"
	},
	"include": [
		"**/*.d.ts",
		"**/*.ts",
		"**/*.tsx"
	],
	"exclude": ["node_modules"]
}
